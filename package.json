{
  "name": "flurish",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "generate-graphql": "gql-gen --config app-gql-gen.yml && gql-gen --config auth-gql-gen.yml",
    "generate-server-js": "ncc build app/javascript/server/machine.ts --out app/javascript/server/generated/machine",
    "generate-state-docs": "ts-node --files -P tsconfig.server.json app/javascript/server/writeDefaultStateDocuments.ts",
    "generate-fixtures": "ts-node --files -P tsconfig.server.json app/javascript/server/writeScopeFixtures.ts",
    "lint": "tsc --noEmit && prettier --check \"app/javascript/**/*.{js,ts,tsx}\" && eslint --ext js,ts,tsx app/javascript",
    "lintfix": "prettier --write \"app/javascript/**/*.{js,ts,tsx}\" && eslint --ext js,ts,tsx --fix app/javascript",
    "test": "jest",
    "open-cypress": "cypress open --project ./test",
    "analyze-bundle": "NODE_ENV=production bin/webpack --profile --json > tmp/webpack-stats.json && webpack-bundle-analyzer tmp/webpack-stats.json"
  },
